def solution():
    numbers = input().strip().split()
    
    if not numbers:
        return
    
    # Convert strings to integers
    numbers = list(map(int, numbers))
    
    # Sort the list
    numbers.sort()
    
    n = len(numbers)
    
    # Median index using (n - 1) // 2 to match the example output
    median = numbers[(n - 1) // 2]
    
    # Count how many times the median appears
    count = numbers.count(median)
    
    print(median, count)


if __name__ == "__main__":
    solution()
